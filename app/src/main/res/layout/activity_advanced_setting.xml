    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/colorLightBlue"
        app:popupTheme="@style/AppTheme.NoActionBar" >

    </androidx.appcompat.widget.Toolbar>
    <LinearLayout
        android:id="@+id/view_cantact"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center"

            android:src="@drawable/call_icon"
            android:layout_width="20dp"
            android:layout_height="20dp" />
        <TextView
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:textSize="18sp"
            android:textAllCaps="false"
            android:text="Who can View &amp; Contact me"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/call_preferences"
        android:layout_marginTop="20dp"
        android:layout_gravity="center"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_gravity="center"

            android:layout_marginLeft="20dp"

            android:src="@drawable/call_icon"
            android:layout_width="20dp"
            android:layout_height="20dp" />
        <TextView
            android:layout_gravity="center"

            android:layout_marginLeft="20dp"

            android:textSize="18sp"
            android:textAllCaps="false"
            android:text="Call Preferences"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="20dp"
        android:layout_gravity="center"
        android:id="@+id/blocked_profiles"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center"

            android:src="@drawable/blocked_profile"
            android:layout_width="20dp"
            android:layout_height="20dp" />
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center"
            android:textSize="18sp"
            android:textAllCaps="false"
            android:text="Blocked Profiles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ignored_profiles"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center"

            android:src="@drawable/ignored_profiles"
            android:layout_width="20dp"
            android:layout_height="20dp" />
        <TextView
            android:layout_gravity="center"

            android:layout_marginLeft="20dp"
            android:textSize="18sp"
            android:textAllCaps="false"
            android:text="Ignored Profiles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/action_confirmation"
        android:layout_marginTop="20dp"
        android:layout_gravity="center"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"

            android:src="@drawable/lock_icon"
            android:layout_width="20dp"
            android:layout_height="20dp" />
        <TextView
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:textSize="18sp"
            android:textAllCaps="false"
            android:text="Action Confirmation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear_actionConfoirmation"
        android:visibility="gone"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_gravity="center"
            android:layout_weight="1"
            android:textSize="16sp"
            android:padding="5dp"
            android:layout_marginTop="10dp"
            android:textColor="@color/colorBlack"
            android:textAllCaps="false"
            android:text="Confirm before sending interest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <Switch
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
</LinearLayout>