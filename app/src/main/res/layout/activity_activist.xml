<View
    android:background="@color/colorGrey"
    android:layout_height="1dp"
    android:layout_width="match_parent"/>

<LinearLayout

    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout

        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="1"
            android:orientation="vertical"
            android:id="@+id/img_audios"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_marginTop="50dp"
                android:layout_gravity="center"
                android:src="@drawable/audio"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:layout_gravity="center"
                android:id="@+id/text_audios"
                android:text="Audios"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginLeft="10dp"
                android:layout_marginTop="50dp"
                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:id="@+id/img_news"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:src="@drawable/news_blue"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:layout_gravity="center"
                android:id="@+id/text_news"
                android:text="News"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginTop="50dp"
                android:layout_marginLeft="10dp"

                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:id="@+id/img_reviews"

            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:src="@drawable/rating_purple"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:layout_gravity="center"
                android:id="@+id/text_reviews"
                android:text="Reviews"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginTop="30dp"
                android:layout_marginLeft="10dp"

                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:id="@+id/feed"

            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:src="@drawable/feed_icon"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:layout_gravity="center"
                android:id="@+id/text_feed"
                android:text="Feed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginTop="50dp"
                android:layout_marginLeft="10dp"

                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>

    </LinearLayout>
    <View
        android:background="@color/colorGrey"
        android:layout_width="1dp"
        android:layout_height="match_parent"/>
    <LinearLayout
        android:layout_gravity="center"
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="1"
            android:orientation="vertical"
            android:id="@+id/img_video"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:src="@drawable/video"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:layout_gravity="center"
                android:id="@+id/text_videos"
                android:text="Videos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginRight="10dp"
                android:layout_marginTop="50dp"
                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:id="@+id/img_auditions"

            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:src="@drawable/audition_icon"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:layout_gravity="center"
                android:id="@+id/text_auditions"
                android:text="Auditions"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginTop="50dp"
                android:layout_marginRight="10dp"

                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:id="@+id/img_post_talent"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:src="@drawable/talent_icon"
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:textAllCaps="false"
                android:text="Post Talent"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginTop="30dp"
                android:layout_marginRight="10dp"

                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"

            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:layout_width="30dp"
                android:layout_height="30dp" />
            <TextView
                android:textAllCaps="false"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_marginTop="50dp"
                android:layout_marginRight="10dp"

                android:background="@color/colorGrey"
                android:layout_height="1dp"
                android:layout_width="match_parent"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
